{
  "manifest_version": 3,
  "name": "URL Logger",
  "version": "1.0",
  "description": "Logs every URL you visit",
  "author": "Kevin Fischer",
  "browser_specific_settings": {
    "gecko": {
      "id": "url_logger@my_private_extension.com"
    }
  },
  "permissions": [
    "nativeMessaging",
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
